@model List<AppUser>


@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<table class="table table-bordered table-striped">

    <tr>
        <td>User Id</td>
        <td>User Name</td>
        <td>Email</td>
    </tr>
    @if (Model.Count() == 0)
    {
        <tr>
            <td colspan="3">Hiçbir üye yoktur.</td>
        </tr>
    }
    else
    {
        @foreach (var item in Model)
        {
            <tr>@item.Id</tr>
            <tr>@item.UserName</tr>
            <tr>@item.Email</tr>
        }
    }
</table>